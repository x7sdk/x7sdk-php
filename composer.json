{
    "name": "x7sy/x7",
    "description": "x7sy php sdk for x7mall, x7Detection etc.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jiangwen",
            "email": "jiangwen@shangminet.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "X7\\": "src/X7/"
        }
    },
    "require": {
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.5|^9.5",
        "phpunit/php-code-coverage": "^7.0|^8.0|^9.2"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage --coverage-clover coverage.xml --log-junit test-results.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "non-feature-branches": ["dev-*", "feat-*", "bugfix", "test-*", "patch-*"]
    }
}
